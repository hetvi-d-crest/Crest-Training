   ],
   "source": [
    "from collections import Counter\n",
    "\n",
    "string = \"Python Exercise!\"\n",
    "counter = Counter(string)\n",
    "\n",
    "print(counter)\n",
    "for letter, count in counter.items():\n",
    "    print(f'{letter} : {count}')"
   ]
