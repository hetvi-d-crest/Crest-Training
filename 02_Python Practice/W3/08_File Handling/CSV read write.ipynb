   "source": [
    "import csv\n",
    "with open('departments.csv', newline = '') as csvfile:\n",
    "    data = csv.reader(csvfile, delimiter=' ', quotechar='|')\n",
    "    for row in data:\n",
    "        print(', '.join(row))"
