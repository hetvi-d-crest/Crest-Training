   ],
   "source": [
    "import json\n",
    "\n",
    "data = '{\"Name\" :\"Geeks\" , \"ID\" : \"2\", \"Location\" : \"Mumbai\"}'\n",
    "py_obj = json.loads(data)\n",
    "print(py_obj)\n",
    "print(\"Name: \", py_obj[\"Name\"])\n",
    "print(\"ID: \", py_obj[\"ID\"])\n",
    "print(\"Location: \", py_obj[\"Location\"])"
   ]
